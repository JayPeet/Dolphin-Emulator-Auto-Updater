<Window x:Class="DolphinAutoUpdater.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DolphinAutoUpdater"
        mc:Ignorable="d"
        Title="Dolphin Auto Updater" Height="128" Width="525" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
    <Grid>
        <TabControl x:Name="tabControl" HorizontalAlignment="Left" Height="99" VerticalAlignment="Top" Width="519">
            <TabItem Header="Updater">
                <Grid Background="#FFE5E5E5" Margin="0,0,-1,10">
                    <ProgressBar x:Name="dl_bar" HorizontalAlignment="Left" Height="16" Margin="10,40,0,0" VerticalAlignment="Top" Width="494" Value="50"/>
                    <Label x:Name="info_label" Content="Info Label" HorizontalAlignment="Left" Margin="10,9,0,0" VerticalAlignment="Top" Width="494"/>
                </Grid>
            </TabItem>
            <TabItem Header="Configure">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
        </TabControl>
    </Grid>
</Window>
